{
    "_version": "1.12.0",
    "sap.app": {
      "id": "intern2020",
      "type": "application",
      "i18n": "i18n/i18n.properties",
      "title": "{{appTitle}}",
      "description": "{{appDescription}}",
      "applicationVersion": {
        "version": "1.0.0"
      },
      "dataSources": {
        "booksService": {
          "uri": "/sap/opu/odata/sap/ZTRAVEL_SRV/",
          "type": "OData",
          "settings": {
            "odataVersion": "2.0"
          }
        }
      }
    },
    "sap.ui": {
      "technology": "UI5",
      "deviceTypes": {
          "desktop": true,
          "tablet": true,
          "phone": true
      }
    },
    "sap.ui5": {
      "rootView": {
          "viewName": "intern2020.view.App",
          "type": "XML",
          "async": true,
          "id": "app"
      },
      "dependencies": {
        "minUI5Version": "1.60",
        "libs": {
          "sap.m": {}
        }
      },
      "models": {
        "i18n": {
          "type": "sap.ui.model.resource.ResourceModel",
          "settings": {
            "bundleName": "intern2020.i18n.i18n",
            "supportedLocales": [""],
            "fallbackLocale": ""
          }
        },
        "": {
            "dataSource": "booksService",
            "preload": true,
            "settings": {
              "metadataUrlParams": {
                "sap-documentation": "heading"
              },
              "defaultBindingMode": "TwoWay"
            }
          }
      },
      "routing": {
        "config": {
            "routerClass": "sap.m.routing.Router",
            "viewPath": "intern2020.view",
            "viewType": "XML",
            "controlId": "appControl",
            "controlAggregation": "pages",
            "bypassed": {
                "target": "notFound"
            },
            "async": true
        },
        "routes": [
            {
                "pattern": "",
                "name": "login",
                "target": "login"
            },
            {
              "pattern": "dashboard",
              "name": "dashboard",
              "target": "dashboard"
            },
            {
              "pattern": "dashboardManager",
              "name": "dashboardManager",
              "target": "dashboardManager"
            },
            {
              "pattern": "userApproved",
              "name": "userApproved",
              "target": "userApproved"
            },
            {
              "pattern": "userDenied",
              "name": "userDenied",
              "target": "userDenied"
            },
            {
              "pattern": "managerDenied",
              "name": "managerDenied",
              "target": "managerDenied"
            },
            {
              "pattern": "managerApproved",
              "name": "managerApproved",
              "target": "managerApproved"
            },
            {
              "pattern": "managerToBeApproved",
              "name": "managerToBeApproved",
              "target": "managerToBeApproved"
            },
            {
              "pattern": "detailApproved/{employeeName}",
              "name": "detailApproved",
              "target": "detailApproved"
            },
            {
              "pattern": "detailToBeApproved",
              "name": "detailToBeApproved",
              "target": "detailToBeApproved"
            },
            {
              "pattern": "detailDenied",
              "name": "detailDenied",
              "target": "detailDenied"
            }
            
        ],
        "targets": {
            "login": {
                "viewName": "Login",
                "viewId": "login",
                "viewLevel": 1
          }, 
          "dashboard": {
            "viewName": "Dashboard",
            "viewId": "dashboard",
            "viewLevel": 1
          },
          "dashboardManager": {
            "viewName": "DashboardManager",
            "viewId": "dashboardManager",
            "viewLevel": 1
          },
          "userApproved": {
            "viewName": "UserApproved",
            "viewId": "userApproved",
            "viewLevel": 1
          },
          "userDenied": {
            "viewName": "UserDenied",
            "viewId": "userDenied",
            "viewLevel": 1
          },
          "managerApproved": {
            "viewName": "ManagerApproved",
            "viewId": "managerApproved",
            "viewLevel": 1
          },
          "managerDenied": {
            "viewName": "ManagerDenied",
            "viewId": "managerDenied",
            "viewLevel": 1
          },
          "managerToBeApproved": {
            "viewName": "managerToBeApproved",
            "viewId": "managerToBeApproved",
            "viewLevel": 1
          },
          "detailApproved": {
            "viewName": "DetailApproved",
            "viewId": "detailApproved",
            "viewLevel": 1
          },
          "detailToBeApproved": {
            "viewName": "DetailToBeApproved",
            "viewId": "detailToBeApproved",
            "viewLevel": 1
          },
          "detailDenied": {
            "viewName": "DetailDenied",
            "viewId": "detailDenied",
            "viewLevel": 1
          }
        }
      },
      "resources": {
        "css": [
        {
          "uri": "CSS/loginStyle.css"
        }
        ]
      }
    }
  }